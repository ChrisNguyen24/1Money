<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sqlitedemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_twotone_account_balance_wallet_24"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SQLiteDemo"
        android:usesCleartextTraffic="true">
        <activity android:name=".Money.ActivityAddWallet"></activity>
        <activity android:name=".menu.ActivityContact" />
        <activity android:name=".extention.ActivityTax" />
        <activity android:name=".extention.ActivityCovid" />
        <activity android:name=".Money.ActivitySelectMoney" />
        <activity android:name=".Money.ActivityNewMoney" />
        <activity android:name=".Note.ActivitySelectedItem" />
        <activity android:name=".Note.ActivityCreateStudent" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".MyReceiver"
            android:exported="false" />
    </application>

</manifest>